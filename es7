#include <stdio.h>

int main(){
    int a,b,i;
    printf("Scegli primo numero:\n");
    scanf("%d",&a);
    
    printf("Scegli secondo numero:\n");
    scanf("%d",&b);
    
    for(i=1;(i<=a)||(i<=b);i++) {
        if((a%i==0)&&(b%i==0)){
            printf("%d ",i);
        }
    }
    printf("sono i divisori comuni di %d e %d\n",a,b);
        return 0;
}
